{
    "author": "Bertrand Thierry <bthierry@math.cnrs.fr>",
    "bugs": {
        "url": "https://github.com/Bertbk/vscode-gmsh/issues/"
      },
    "name": "vscode-gmsh",
    "displayName": "vscode-gmsh",
    "description": "GMSH language support in Visual Studio Code",
    "version": "0.1.2",
    "keywords": [
        "gmsh",
        "language"
    ],
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gmsh",
            "aliases": ["GMSH", "gmsh"],
            "extensions": [".geo"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "gmsh",
            "scopeName": "source.geo",
            "path": "./syntaxes/gmsh.tmLanguage.json"
        }]
    },
    "icon": "logos/gmsh-logo.png",
    "homepage": "https://gmsh.info/",
    "license": "MIT",
    "publisher": "Bertrand-Thierry",
    "repository": {
        "type": "git",
        "url": "https://github.com/Bertbk/vscode-gmsh.git"
    }
}